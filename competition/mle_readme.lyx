\begin_inset Formula 
\begin{eqnarray*}
\Delta TD_{jm} & = & \gamma_{m}\left(\frac{\sum_{k=1}^{J_{jm}^{R}}\sum_{p=0}^{J_{jm}^{D}}\left(_{J_{jm}^{R}-1}C_{k-1}\right)\left(_{J_{jm}^{D}}C_{p}\right)exp\left(\left(k+p\right)\alpha+k\beta+_{\left(k+p\right)}C_{2}\Gamma\right)}{\sum_{k=0}^{J_{jm}^{R}}\sum_{p=0}^{J_{jm}^{D}}\left(_{J_{jm}^{R}}C_{k}\right)\left(_{J_{jm}^{D}}C_{p}\right)exp\left(\left(k+p\right)\alpha+k\beta+_{\left(k+p\right)}C_{2}\Gamma\right)}\right)\\
 &  & +(1-\gamma_{i})\left(\frac{\sum_{k=1}^{J_{jm}^{R}}\sum_{p=0}^{J_{jm}^{D}}\left(_{J_{jm}^{R}-1}C_{k-1}\right)\left(_{J_{jm}^{D}}C_{p}\right)exp\left(\left(k+p\right)\alpha+p\beta+_{\left(k+p\right)}C_{2}\Gamma\right)}{\sum_{k=0}^{J_{jm}^{R}}\sum_{p=0}^{J_{jm}^{D}}\left(_{J_{jm}^{R}}C_{k}\right)\left(_{J_{jm}^{D}}C_{p}\right)exp\left(\left(k+p\right)\alpha+p\beta+_{\left(k+p\right)}C_{2}\Gamma\right)}\right)\\
 &  & -\gamma_{i}\left(\frac{\sum_{k=1}^{J_{jm}^{D}}\sum_{p=0}^{J_{jm}^{R}}\left(_{J_{jm}^{D}-1}C_{k-1}\right)\left(_{J_{jm}^{R}}C_{p}\right)exp\left(\left(k+p\right)\alpha+p\beta+_{\left(k+p\right)}C_{2}\Gamma\right)}{\sum_{k=0}^{J_{jm}^{D}}\sum_{p=0}^{J_{jm}^{R}}\left(_{J_{jm}^{D}}C_{k}\right)\left(_{J_{jm}^{R}}C_{p}\right)exp\left(\left(k+p\right)\alpha+p\beta+_{\left(k+p\right)}C_{2}\Gamma\right)}\right)\\
 &  & -(1-\gamma_{i})\left(\frac{\sum_{k=1}^{J_{jm}^{D}}\sum_{p=0}^{J_{jm}^{R}}\left(_{J_{jm}^{D}-1}C_{k-1}\right)\left(_{J_{jm}^{R}}C_{p}\right)exp\left(\left(k+p\right)\alpha+k\beta+_{\left(k+p\right)}C_{2}\Gamma\right)}{\sum_{k=0}^{J_{jm}^{D}}\sum_{p=0}^{J_{jm}^{R}}\left(_{J_{jm}^{D}}C_{k}\right)\left(_{J_{jm}^{R}}C_{p}\right)exp\left(\left(k+p\right)\alpha+k\beta+_{\left(k+p\right)}C_{2}\Gamma\right)}\right)
\end{eqnarray*}

\end_inset


\end_layout

